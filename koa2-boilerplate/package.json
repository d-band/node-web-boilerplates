{
  "name": "koa2-boilerplate",
  "version": "0.0.1",
  "description": "koa2 boilerplate",
  "main": "app/index.js",
  "scripts": {
    "start": "node app/index.js",
    "build": "rimraf app && BABEL_ENV=production babel --out-dir=app src",
    "test": "nyc mocha --compilers js:babel-register --no-timeouts",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext .js src"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/node-web-boilerplates.git"
  },
  "keywords": [
    "koa2",
    "boilerplate",
    "nodejs",
    "web"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/node-web-boilerplates/issues"
  },
  "homepage": "https://github.com/d-band/node-web-boilerplates#readme",
  "dependencies": {
    "co": "~4.6.0",
    "koa": "~2.0.0",
    "koa-bodyparser": "~3.1.0",
    "koa-convert": "~1.2.0",
    "koa-logger": "~2.0.0",
    "koa-router": "~7.0.1",
    "koa-view": "~0.0.3"
  },
  "devDependencies": {
    "babel-cli": "~6.11.4",
    "babel-eslint": "~6.1.2",
    "babel-plugin-transform-async-to-generator": "~6.8.0",
    "babel-preset-es2015-node4": "~2.1.0",
    "babel-register": "~6.11.6",
    "chai": "~3.5.0",
    "chai-http": "~3.0.0",
    "coveralls": "~2.11.9",
    "eslint": "~3.3.1",
    "eslint-config-standard": "~6.0.0-beta.3",
    "eslint-plugin-promise": "~2.0.1",
    "eslint-plugin-standard": "~2.0.0",
    "mocha": "~3.0.2",
    "nyc": "~8.1.0",
    "pre-commit": "~1.1.3",
    "rimraf": "~2.5.4"
  }
}